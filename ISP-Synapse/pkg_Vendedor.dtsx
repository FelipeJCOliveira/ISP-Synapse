        '
        '-- INICIO
        'profile = "false"
        'landingPadDir = Dts.Variables("strLandingPadDir").Value.ToString
        'resultsDir = Dts.Variables("strResultsDir").Value.ToString
        'user = Dts.Variables("strUserName").Value.ToString
        'pass = Dts.Variables("strPassword").Value.ToString
        'driveLetter = Dts.Variables("strDriveLetter").Value.ToString

        'Dim profile As String
        'profile = "false"
        'resultsDir = Dts.Variables("strResultsDir").Value.ToString
        Dim driveLetter, user, pass, landingPadDir As String
        driveLetter = Dts.Variables("FSMapDrive").Value.ToString ' "S:"
        user = Dts.Variables("FSMapUser").Value.ToString '"H975835"
        pass = Dts.Variables("FSMapPass").Value.ToString '"SyngentaAgro01SyngentaAgro01"
        landingPadDir = Dts.Variables("FSMapDir").Value.ToString '"\\BRSPFPC01VFIL1\Custom"

        Dim objNetwork As Object
        objNetwork = CreateObject("WScript.Network")
        Dim CheckDrive As Object
        CheckDrive = objNetwork.EnumNetworkDrives()
        Dim intcount As Integer
        Dim achouDrive As Boolean

        achouDrive = False

        If CheckDrive.Count > 0 Then
            For intcount = 0 To CheckDrive.Count - 1 Step 2 'if drive is already mapped, then disconnect it
                If CheckDrive.Item(intcount) = driveLetter Then
                    If CheckDrive.Item(intcount + 1) <> landingPadDir Then

                        objNetwork.RemoveNetworkDrive(driveLetter)
                    Else
                        achouDrive = True
                    End If
                End If
            Next
        End If
        If Not achouDrive Then
            objNetwork.MapNetworkDrive(driveLetter, landingPadDir, False, user, pass)
        End If
        '-- FIM
        Dts.TaskResult = ScriptResults.Success
    End Sub

